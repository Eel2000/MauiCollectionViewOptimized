<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiCollectionViewOptimized.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MauiCollectionViewOptimized"
    Title="MauiCollectionViewOptimized"
    Shell.FlyoutBehavior="Flyout">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="Vr-List"
        ContentTemplate="{DataTemplate local:WithVirtualList}"
        Route="VrList" />

</Shell>
